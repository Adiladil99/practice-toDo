{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "plugins": ["jest"],
    "extends": [
        "eslint:recommended", "google"
    ],
    "rules": {
        "semi": "off",
        "no-console": "off",
        "indent": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "eol-last": "off",
        "require-jsdoc": "off",
        "no-trailing-spaces": "off",
        "operator-linebreak": "off",
        "linebreak-style": "off",
        "max-len": "off"
    }
}
